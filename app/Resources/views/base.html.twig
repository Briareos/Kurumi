<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheet %}
        <link href="//yui.yahooapis.com/3.4.1/build/cssreset/cssreset-min.css" rel="stylesheet" type="text/css" />
        <link href="{{ asset('library/tipsy/src/stylesheets/tipsy.css') }}" rel="stylesheet" type="text/css" />
        <link href="//94.189.249.111:1337/css/screen.less" type="text/css" rel="stylesheet" />

        {% endblock %}
        {% block javascript_library %}
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

        {% endblock %}
        {% block javascript_plugin %}
        <script src="{{ asset('library/jquery.tipsy/src/javascripts/jquery.tipsy.js') }}" type="text/javascript"></script>
        <script src="{{ asset('library/jquery.form/jquery.form.js') }}" type="text/javascript"></script>
        <script src="{{ asset('library/jquery.cookie/jquery.cookie.js') }}" type="text/javascript"></script>

        {% endblock %}
        {% block javascript_header %}

        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block page_top %}{% endblock %}
        {% block page %}{% endblock %}
        {% block page_bottom %}{% endblock %}
        {% block javascript_footer %}{% endblock %}
    </body>
</html>
