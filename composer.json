{
    "name":"symfony/framework-standard-edition",
    "autoload":{
        "psr-0":{
            "":"src/"
        }
    },
    "require":{
        "php":">=5.3.2",
        "symfony/symfony":"2.1.*",
        "doctrine/orm":"2.2.0",
        "doctrine/doctrine-bundle":"dev-master",
        "twig/extensions":"dev-master",
        "symfony/assetic-bundle":"dev-master",
        "symfony/swiftmailer-bundle":"dev-master",
        "symfony/monolog-bundle":"dev-master",
        "sensio/distribution-bundle":"dev-master",
        "sensio/framework-extra-bundle":"dev-master",
        "sensio/generator-bundle":"dev-master",
        "jms/security-extra-bundle":"1.1.0",
        "jms/di-extra-bundle":"1.0.1",
        "doctrine/doctrine-fixtures-bundle":"dev-master",
        "lunetics/locale-bundle":"dev-master",
        "knplabs/knp-zend-cache-bundle":"dev-master",
        "friendsofsymfony/facebook-bundle":"dev-master",
        "facebook/php-sdk":"dev-master",
        "stof/doctrine-extensions-bundle":"dev-master",
        "sonata-project/admin-bundle":"dev-master",
        "sonata-project/doctrine-orm-admin-bundle":"dev-master"
    },
    "repositories":{
        "zendframework/zend-cache":{
            "type":"package",
            "package":{
                "name":"zendframework/zend-cache",
                "version":"2.0.0beta1",
                "dist":{
                    "url":"http://github.com/KnpLabs/zend-cache/zipball/master",
                    "type":"zip"
                },
                "source":{
                    "url":"git://github.com/KnpLabs/zend-cache.git",
                    "type":"git",
                    "reference":"master"
                },
                "autoload":{
                    "psr-0":{
                        "Zend\\Cache":""
                    }
                },
                "target-dir":"Zend/Cache"
            }
        },
        "zendframework/zend-filter":{
            "type":"package",
            "package":{
                "name":"zendframework/zend-filter",
                "version":"2.0.0beta1",
                "dist":{
                    "url":"http://github.com/KnpLabs/zend-filter/zipball/master",
                    "type":"zip"
                },
                "source":{
                    "url":"git://github.com/KnpLabs/zend-filter.git",
                    "type":"git",
                    "reference":"master"
                },
                "autoload":{
                    "psr-0":{
                        "Zend\\Filter":""
                    }
                },
                "target-dir":"Zend/Filter"
            }
        }
    },
    "scripts":{
        "post-install-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "config":{
        "bin-dir":"bin"
    },
    "extra":{
        "symfony-app-dir":"app",
        "symfony-web-dir":"webroot"
    }
}
